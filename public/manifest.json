{
  "manifest_version": 3,
  "name": "Kiss Flow",
  "version": "1.0",
  "action": {
    "default_title": "Kiss Flow"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": ["activeTab", "tabs", "storage", "history", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["contentScript.bundle.js"]
    }
  ]
}
